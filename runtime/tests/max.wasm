%core "0100"
%name "Test max"
%version "1"
%class "black"

.Test_MAX
    ; init
    mov   [48], 0
    mov   [49], 1

    ; 1
    ; max (5, 1) == 1
    inc   [48]
    mov   R0, 5 
    mov   r1, 1
    max   r0, r1
    eq    r0, 5
    jf    abort
    
    ; 2
    ; max (2, 6) == 2
    inc   [48]
    mov   R0, 2
    mov   r1, 6
    max   r0, r1
    eq    r0, 6
    jf    abort
    
    
    ; 3
    ; max (<4, 2>, 6) == <4, 2>
    inc   [48]
    mov   p0:x, 4
    mov   p0:y, 2
    max   p0, 6
    eq    p0:x, 6
    jf    abort
    eq    p0:y, 6
    jf    abort
    
    ; 4
    ; max (<4, 8>, 6) == <4, 6>
    inc   [48]
    mov   p0:x, 4
    mov   p0:y, 8
    max   p0, 6
    eq    p0:x, 6
    jf    abort
    eq    p0:y, 8
    jf    abort
    
    ; 5
    ; max (<7, 8>, 6) == <6, 6>
    inc   [48]
    mov   p0:x, 7
    mov   p0:y, 8
    max   p0, 6
    eq    p0:x, 7
    jf    abort
    eq    p0:y, 8
    jf    abort
    
    ; 6
    ; max (<9, 15>, <13, 3>) == <9, 3>
    inc   [48]
    mov   p0:x, 9
    mov   p0:y, 15
    mov   p1:x, 13
    mov   p1:y, 3
    max   p0, p1
    eq    p0:x, 13
    jf    abort
    eq    p0:y, 15
    jf    abort

    ; 7
    ; max (<14, 22>, <20, 20>) == <14, 20>
    inc   [48]
    mov   [50], 14
    mov   [51], 22
    mov   p0:x, 20
    mov   p0:y, p0:x
    max   <50>, p0
    eq    [50], 20
    jf    abort
    eq    [51], 22
    jf    abort

    exit
    
abort:
    mov   [49], 0
    exit

.worker
    call Test_MAX
    EXIT

.solder
    EXIT

.queen
    EXIT
